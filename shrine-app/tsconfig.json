{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "target": "ES2021",
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
